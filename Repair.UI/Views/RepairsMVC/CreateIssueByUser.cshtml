@model Repair.UI.Models.DTOsMVC.RepairDTO.RepairsForUserDTOMVC


<form method="post">

    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="RepairName" />
    </div>
    <div class="mt-3">
        <label class="form-label">Description</label>
        <input type="text" class="form-control" asp-for="DescriptionRepair" />
    </div>
    

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Send</button>
    </div>

</form>
